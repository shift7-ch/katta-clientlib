{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::430118840017:role/testing.katta.cloud-kc-realms-chipotle-sts-chain-01"
            },
            "Action": [
                "sts:AssumeRole",
                "sts:TagSession"
            ],
            "Condition": {
                "ForAnyValue:StringEquals": {
                    "sts:TransitiveTagKeys": "${aws:RequestTag/VaultRequested}"
                }
            }
        }
    ]
}
